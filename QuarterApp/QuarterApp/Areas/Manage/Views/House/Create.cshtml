@model House
<!-- partial -->
<div class="main-panel">
    <div class="content-wrapper">
        <div class="page-header">
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item">Tables</li>
                    <li class="breadcrumb-item">House Table</li>
                    <li class="breadcrumb-item active" aria-current="page">Create</li>
                </ol>
            </nav>
        </div>
        <div class="row">
            <div class="col-lg-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Create</h4>
                        <form asp-action="create" method="post" enctype="multipart/form-data">
                            <div class="form-group">
                                <label asp-for="Name">Name</label>
                                <input asp-for="Name" class="form-control">
                                <span class="text-danger" asp-validation-for="Name"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="CityId">City</label>
                                <select asp-for="CityId" class="form-control" asp-items="@(new SelectList(ViewBag.City,nameof(City.Id),nameof(City.Name)))">
                                </select>
                                <span class="text-danger" asp-validation-for="CityId"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="CategoryId">Category</label>
                                <select asp-for="CategoryId" class="form-control" asp-items="@(new SelectList(ViewBag.Category,nameof(Category.Id),nameof(Category.Name)))">
                                </select>
                                <span class="text-danger" asp-validation-for="CategoryId"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="ManagerId">Manager</label>
                                <select asp-for="ManagerId" class="form-control" asp-items="@(new SelectList(ViewBag.Manager,nameof(SaleManager.Id),nameof(SaleManager.Fullname)))">
                                </select>
                                <span class="text-danger" asp-validation-for="ManagerId"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="AmenityIds">Amenities</label>
                                <select asp-for="AmenityIds" class="form-control" asp-items="@(new SelectList(ViewBag.Amenities,nameof(Amenity.Id),nameof(Amenity.Name)))">
                                </select>
                                <span class="text-danger" asp-validation-for="AmenityIds"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Description">Description</label>
                                <textarea asp-for="Description" class="form-control"></textarea>
                                <span class="text-danger" asp-validation-for="Description"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="SalePrice">SalePrice</label>
                                <input asp-for="SalePrice" class="form-control">
                                <span class="text-danger" asp-validation-for="SalePrice"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="CostPrice">CostPrice</label>
                                <input asp-for="CostPrice" class="form-control">
                                <span class="text-danger" asp-validation-for="CostPrice"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="DiscountPercantage">DiscountPercantage</label>
                                <input asp-for="DiscountPercantage" class="form-control">
                                <span class="text-danger" asp-validation-for="DiscountPercantage"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="RoomCount">RoomCount</label>
                                <input asp-for="RoomCount" class="form-control">
                                <span class="text-danger" asp-validation-for="RoomCount"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="BedroomCount">BedroomCount</label>
                                <input asp-for="BedroomCount" class="form-control">
                                <span class="text-danger" asp-validation-for="BedroomCount"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="BathroomCount">BathroomCount</label>
                                <input asp-for="BathroomCount" class="form-control">
                                <span class="text-danger" asp-validation-for="BathroomCount"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Area">Area</label>
                                <input asp-for="Area" class="form-control">
                                <span class="text-danger" asp-validation-for="Area"></span>
                            </div>
                            <div class="form-group d-inline-block">
                                <label asp-for="IsNew ">IsNew</label>
                                <input asp-for="IsNew" type="checkbox" class="">
                                <span class="text-danger" asp-validation-for="IsNew"></span>
                            </div>
                            <div class="form-group d-inline-block">
                                <label asp-for="IsSpecial">IsSpecial</label>
                                <input asp-for="IsSpecial">
                                <span class="text-danger" asp-validation-for="IsSpecial"></span>
                            </div>
                            <div class="form-group d-inline-block">
                                <label asp-for="StockStatus">StockStatus</label>
                                <input asp-for="StockStatus">
                                <span class="text-danger" asp-validation-for="StockStatus"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="BuildYear">BuildYear</label>
                                <input asp-for="BuildYear" class="form-control">
                                <span class="text-danger" asp-validation-for="BuildYear"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="PosterImage">PosterImage</label>
                                <input asp-for="PosterImage" class="form-control">
                                <span class="text-danger" asp-validation-for="PosterImage"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="ImageFiles">ImageFiles</label>
                                <input asp-for="ImageFiles" class="form-control">
                                <span class="text-danger" asp-validation-for="ImageFiles"></span>
                            </div>
                            <button type="submit" class="btn btn-gradient-primary">Create</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- main-panel ends -->
@section Scripts{
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.5/jquery.validate.min.js" integrity="sha512-rstIgDs0xPgmG6RX1Aba4KV5cWJbAMcvRCVmglpam9SoHZiUCyQVDdH2LPlxoHtrv17XWblE/V/PP+Tr04hbtA==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/4.0.0/jquery.validate.unobtrusive.min.js" integrity="sha512-xq+Vm8jC94ynOikewaQXMEkJIOBp7iArs3IhFWSWdRT3Pq8wFz46p+ZDFAR7kHnSFf+zUv52B3prRYnbDRdgog==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
                        }