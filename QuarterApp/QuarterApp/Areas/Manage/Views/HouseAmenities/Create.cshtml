@model HouseAmenity
<!-- partial -->
<div class="main-panel">
    <div class="content-wrapper">
        <div class="page-header">
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item">Tables</li>
                    <li class="breadcrumb-item">HouseAmenities Table</li>
                    <li class="breadcrumb-item active" aria-current="page">Create</li>
                </ol>
            </nav>
        </div>
        <div class="row">
            <div class="col-lg-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <form asp-action="create" method="post">
                            <div class="form-group">
                                <label asp-for="AmenityId">Amenity</label>
                                <select asp-for="AmenityId" class="form-control" asp-items="@(new SelectList(ViewBag.Amenity,nameof(Amenity.Id),nameof(Amenity.Name)))">
                                </select>
                                <span class="text-danger" asp-validation-for="AmenityId"></span>
                            </div>                            <div class="form-group">
                                <label asp-for="HouseId">House</label>
                                <select asp-for="HouseId" class="form-control" asp-items="@(new SelectList(ViewBag.House,nameof(House.Id),nameof(House.Name)))">
                                </select>
                                <span class="text-danger" asp-validation-for="HouseId"></span>
                            </div>
                            <button type="submit" class="btn btn-gradient-primary">Create</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- main-panel ends -->
